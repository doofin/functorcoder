{
    "name": "functorcoder",
    "displayName": "functorcoder",
    "description": "an ai coding assistant",
    "repository": {
        "type": "git",
        "url": "https://github.com/doofin/functorcoder/"
    },
    "version": "0.0.1",
    "publisher": "functorcoder.com",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "engines": {
        "vscode": "^1.84.0"
    },
    "dependencies": {
        "node-fetch": "2.6.0",
        "vscode-languageclient": "9.0.1"
    },
    "devDependencies": {
        "@types/vscode": "^1.73.0"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "functorcoder",
            "properties": {
                "openaiApiKey": {
                    "type": "string",
                    "default": ""
                },
                "openaiUrl": {
                    "type": "string",
                    "default": ""
                }
            }
        },
        "commands": [
            {
                "command": "functorcoder.menu",
                "title": "functorcoder main menu"
            },
            {
                "command": "functorcoder.createFiles",
                "title": "generate files and folders"
            }
        ],
        "menus": {
            "file/newFile": [
                {
                    "command": "functorcoder.menu",
                    "group": "navigation"
                }
            ],
            "editor/context": [
                {
                    "command": "functorcoder.menu",
                    "group": "1_modification"
                }
            ]
        }
    }
}